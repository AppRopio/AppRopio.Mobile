<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/app_products_productCard_horizontalCollection_content_height"
    android:layout_height="@dimen/app_products_productCard_horizontalCollection_content_height">
    <FrameLayout
        android:layout_width="@dimen/app_products_productCard_horizontalCollection_color_size"
        android:layout_height="@dimen/app_products_productCard_horizontalCollection_color_size"
        android:layout_centerInParent="true"
        app:MvxBind="Background ProductsColorBackground(Value)" />
    <ImageView
        style="@style/app_products_productCard_horizontalCollection_color_checkImage"
        android:layout_centerInParent="true"
        app:MvxBind="Visibility Visibility(Selected)" />
</RelativeLayout>