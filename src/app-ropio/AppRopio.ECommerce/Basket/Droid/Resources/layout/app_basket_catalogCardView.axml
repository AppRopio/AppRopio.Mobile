<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <Button
        style="@style/app_basket_catalogCardView_buyButton"
        android:layout_width="match_parent"
        android:layout_height="@dimen/app_basket_catalogCardView_buyButton_height"
        android:layout_marginLeft="@dimen/app_basket_catalogCardView_buyButton_horizontalMargins"
        android:layout_marginRight="@dimen/app_basket_catalogCardView_buyButton_horizontalMargins"
        android:layout_marginTop="@dimen/app_basket_catalogCardView_buyButton_marginTop"
        android:layout_marginBottom="@dimen/app_basket_catalogCardView_buyButton_marginBottom"
        app:MvxBind="Click BuyCommand; Text Resx('BasketCatalogCard_AddToBasket','Basket')" />
</RelativeLayout>